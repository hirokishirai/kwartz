<%@ taglib prefix="c" uri="http://java.sun.com/jstl/core" %>
<html>
  <body>

    <div id="breadcrumbs">
<c:forEach var="crumb" items="${breadcrumbs}">
      <a href="<c:out value="${crumb['path']}" escapeXml="false"/>"><c:out value="${crumb['name']}" escapeXml="false"/></a>
      &gt;
</c:forEach>
      <c:out value="${title}" escapeXml="false"/>
    </div>

  </body>
</html>

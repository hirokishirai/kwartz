#navi {
	plogic: {
	    if (page > 0) {
	        @stag;
	        @cont;
	        @etag;
	    }
	}
}


#navi_first {
	attr:  "href" first_url;
	plogic: {
	    if (first_url != empty) {
	        @stag;
	        @cont;
	        @etag;
	    } else {
	        @cont;
	    }
	}
}



#index {
	plogic: {
	    if (page == 0) {
	        i = 0;
	        foreach (thumb in thumb_list) {
	            i += 1;
	            link_url  = thumb[:link_url];
	            image_url = thumb[:image_url];
	            @element(thumbnail);
	            if (i % 3 == 0) {
	                @element(br);
	            }
	        }
	    }
	    @etag;
	}
}


#thumbnail {
	attr: "href" link_url;
}


#main_image {
	plogic: {
	    if (page != 0) {
	        @stag;
	        @cont;
	        @etag;
	    }
	}
}


#navi2 {
	plogic: {
	    @element(navi);
	}
}

<html>
  <head>
    <title>Kwartz Benchmark</title>
  </head>
  <body>
    <h1>Kwartz Benchmark</h1>

    <table>
#foreach($item in $list)
#if($velocityCount % 2 == 0)
#set($color = "#FFCCCC")
#else
#set($color = "#CCCCFF")
#end
      <tr bgcolor="${color}">
        <td>${item}</td>
      </tr>
#end
    </table>

  </body>
</html>
